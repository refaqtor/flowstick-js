{
  "name": "flowstick",
  "productName": "Flowstick",
  "version": "0.0.1",
  "description": "A XPDL editor",
  "main": "main.js",
  "private": true,
  "scripts": {
    "lint": "eslint src *.js",
    "hot-server": "node server.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron ./",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\""
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "author": {
    "name": "Eric Butler",
    "email": "idealeric@gmail.com"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "electron-packager": "^6.0.0",
    "electron-prebuilt": "^0.37.2",
    "electron-rebuild": "^1.1.3",
    "eslint": "^2.5.3",
    "eslint-plugin-react": "^4.2.3",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.5.0",
    "json-loader": "^0.5.4",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.4.2",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "redux-logger": "^2.6.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "bluebird": "3.3.4",
    "classnames": "^2.2.3",
    "css-modules-require-hook": "^4.0.0",
    "electron-debug": "^0.5.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "immutable": "^3.7.6",
    "react": "^0.14.7",
    "react-addons-pure-render-mixin": "0.14.8",
    "react-canvas": "^1.1.0",
    "react-dom": "^0.14.7",
    "react-draggable": "^1.3.4",
    "react-immutable-proptypes": "1.7.0",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "reselect": "2.2.1",
    "xml2js": "^0.4.16"
  },
  "devEngines": {
    "node": "5.x",
    "npm": "3.x"
  }
}
